<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="2.24"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkAdjustment" id="volume_adj">
    <property name="upper">100</property>
    <property name="value">100</property>
    <property name="step_increment">1</property>
    <property name="page_increment">10</property>
  </object>
  <object class="GtkWindow" id="window1">
    <property name="can_focus">False</property>
    <child>
      <object class="GtkTable" id="volume_control">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <property name="n_rows">3</property>
        <property name="n_columns">2</property>
        <child>
          <object class="GtkToggleButton" id="mute">
            <property name="label" translatable="yes">Silenciar</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
          </object>
          <packing>
            <property name="right_attach">2</property>
            <property name="top_attach">2</property>
            <property name="bottom_attach">3</property>
            <property name="x_options">GTK_SHRINK | GTK_FILL</property>
            <property name="y_options">GTK_SHRINK</property>
          </packing>
        </child>
        <child>
          <object class="GtkProgressBar" id="peak">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="orientation">bottom-to-top</property>
          </object>
          <packing>
            <property name="left_attach">1</property>
            <property name="right_attach">2</property>
            <property name="bottom_attach">2</property>
            <property name="x_options">GTK_SHRINK</property>
          </packing>
        </child>
        <child>
          <object class="GtkVScale" id="volume">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="adjustment">volume_adj</property>
            <property name="inverted">True</property>
            <property name="restrict_to_fill_level">False</property>
            <property name="fill_level">0</property>
            <property name="round_digits">0</property>
            <property name="digits">0</property>
            <property name="value_pos">bottom</property>
          </object>
          <packing>
            <property name="bottom_attach">2</property>
            <property name="x_options">GTK_SHRINK</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
